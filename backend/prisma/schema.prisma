generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Interview {
  id        String   @id @default(uuid())
  userId    String
  role      String
  question  String
  answer    String
  score     Int
  feedback  String
  createdAt DateTime @default(now())
}
